{
    "name": "jesgs/notion",
    "description": "An integration plugin for Notion",
    "require-dev": {
        "wp-coding-standards/wpcs": "^3.1",
        "phing/phing": "^2.17",
        "symfony/dotenv": "^7.0"
    },
    "license": "GPL2",
    "autoload": {
        "psr-4": {
            "JesGs\\Notion\\": "src/"
        },
        "files" : [
          "blocks/callout/callout.php"
        ]
    },
    "authors": [
        {
            "name": "jesgs",
            "email": "2304490+jesgs@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require": {
        "illuminate/collections": "^11.1"
    }
}
